{
  "name": "sema-engine",
  "version": "0.0.1",
  "description": "Module containing Sema's signal engine and compiler for a live coding language ecossystem",
  "repository": "https://github.com/frantic0/sema-engine.git",
  "author": "Francisco Bernardo <f.bernardo@sussex.ac.uk>",
  "license": "MIT",
  "keywords": [
    "Web Audio",
    "Web Audio API",
    "Synthesis",
    "Playback",
    "DSP",
    "Signal Processing",
    "Interactive Music"
  ],
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "private": true,
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "webpack && cp dist/sema-engine.js dist/ringbuf.js dist/maxi-processor.js dist/maximilian.wasmmodule.js examples/",
    "test": "node test/test.js",
    "pretest": "npm run build",
    "clean": "rm -rf build dist",
    "clean-all": "rm -rf .cache build dist node_modules package-lock.json"
  },
  "dependencies": {
    "nearley": "^2.20.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-modules-umd": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "eslint": "^7.17.0",
    "eslint-config-google": "^0.14.0",
    "file-loader": "^6.2.0",
    "mocha": "^7.1.1",
    "webpack": "^5.12.3",
    "webpack-cli": "^4.3.1"
  }
}
