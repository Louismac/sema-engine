{
  "name": "sema-engine",
  "version": "0.0.4",
  "description": "Module containing Sema's signal engine and compiler for a live coding language ecossystem",
  "repository": {
    "type": "git",
    "url": "https://github.com/frantic0/sema-engine.git"
  },
  "author": "Francisco Bernardo <f.bernardo@sussex.ac.uk>",
  "license": "MIT",
  "keywords": [
    "Web Audio",
    "Web Audio API",
    "Synthesis",
    "Playback",
    "DSP",
    "Signal Processing",
    "Interactive Music"
  ],
  "module": "dist/sema-engine.mjs",
  "main": "dist/sema-engine.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c -w",
    "test": "node test/test.js",
    "pretest": "npm run build",
    "clean": "rm -rf build dist",
    "clean-all": "rm -rf .cache dist node_modules package-lock.json"
  },
  "dependencies": {
    "nearley": "^2.20.1",
    "maximilian-wasm": "0.0.2"
  },
  "devDependencies": {
    "rollup": "^2.36.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-wasm": "^5.1.2",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "eslint": "^7.17.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^7.1.1"
  }
}
